<UserControl x:Class="SurvivalGT.InfoUC.MeleeWeaponUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SurvivalGT.InfoUC"
             mc:Ignorable="d" 
             d:DesignHeight="200" d:DesignWidth="400">
    
    <StackPanel>
        <local:WeaponUC DataContext="{Binding Path=WeaponViewModel}"/>

        <local:BreakableUC DataContext="{Binding Path=BreakableViewModel}"/>
    </StackPanel>
</UserControl>
