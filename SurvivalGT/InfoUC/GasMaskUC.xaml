<UserControl x:Class="SurvivalGT.InfoUC.GasMaskUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SurvivalGT.InfoUC"
             mc:Ignorable="d" 
             d:DesignHeight="250" d:DesignWidth="500">

    <StackPanel>
        <local:MaskUC DataContext="{Binding BindsDirectlyToSource=True}"/>

        <local:RepairUC DataContext="{Binding Path=RepairViewModel}"/>
    </StackPanel>
</UserControl>
